{
  "name": "gitbook-plugin-bibtex-citation-michael",
  "description": "Bibtex citations for GitBook",
  "author": "Manuel Mitasch",
  "main": "index.js",
  "version": "1.2.0",
  "engines": {
    "gitbook": ">=2.0.0"
  },
  "homepage": "https://github.com/manuelmitasch/gitbook-plugin-bibtex-citation",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelmitasch/gitbook-plugin-bibtex-citation"
  },
  "license": "MIT",
  "dependencies": {
    "bibtex-parser-js": "0.0.2",
    "lodash": "^4.0.0"
  },
  "scripts": {},
  "gitbook": {
      "properties": {
          "bibtex-path": {
              "type": "string",
              "default": "literature.bib",
              "description": "Path to the BibTeX references file."
          }
      }
  }
}
