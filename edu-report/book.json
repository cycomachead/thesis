{
    "title": "Î» - An Autograder for Snap!",
    "author": "Michael Ball",
    "description": "Prepared for SCMATHE220C Spring 2016",
    "styles": {
        "ebook": "ebook-edu.css"
    },
    "plugins": [
        "bibtex-citation-michael",
        "image-captions"
    ],
    "pluginsConfig": {
        "bibtex": {
            "bibtex-path": "references.bib",
            "references-url": "/references.html",
            "format": "acm"
        },
        "image-captions": {
            "attributes": { "width": "350" },
            "variable_name": "figures",
            "caption": "<strong>Figure _BOOK_IMAGE_NUMBER_:</strong> _CAPTION_",
            "list_caption": "_CAPTION_"
        }
    },
    "structure": {
        "readme": "intro.md"
    },
    "pdf": {
        "toc": true,
        "pageNumbers": false,
        "fontSize": 10,
        "paperSize": "letter",
        "chapterMark": "none",
        "pageBreaksBefore": "/",
        "margin": {
            "right": 60,
            "left": 60,
            "top": 54,
            "bottom": 54
        },
        "headerContent": " ",
        "footerContent": " "
    }
}