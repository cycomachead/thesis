{
    "title": "Î» - An Autograder for Snap!",
    "author": "Michael Ball",
    "description": "An autograder for the Snap! programming language, my UC Berkeley Masters' Thesis.",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/website.css"
    },
    "plugins": [
        "anchors",
        "indexes",
        "bibtex-citation-michael",
        "image-captions"
    ],
    "pluginsConfig": {
        "bibtex": {
            "bibtex-path": "references.bib",
            "references-url": "/references.html",
            "format": "acm"
        },
        "image-captions": {
            "attributes": { "width": "360" },
            "variable_name": "figures",
            "caption": "<strong>Figure _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_:</strong> _CAPTION_",
            "list_caption": "_CAPTION_",
            "images": {
                "8.4": {
                    "width": "600"
                },
                "8.5": {
                    "width": "600",
                    "caption": "<strong>Figure _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_:</strong> _CAPTION_ (Greyed out items mean that the user hasn't done something to enable that action.)"
                }
            }
        }
    },
    "structure": {
        "readme": "abstract.md"
    },
    "pdf": {
        "toc": true,
        "pageNumbers": true,
        "fontSize": 12,
        "paperSize": "letter",
        "chapterMark": "none",
        "pageBreaksBefore": "/",
        "margin": {
            "right": 90,
            "left": 90,
            "top": 90,
            "bottom": 90
        }
    }
}