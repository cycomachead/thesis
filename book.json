{
    "title": "Î» - An Autograder for Snap!",
    "author": "Michael Ball",
    "description": "An autograder for the Snap! programming language, my UC Berkeley Masters' Thesis.",
    "plugins": [
        "bibtex-citation-michael",
        "image-captions"
    ],
    "styles": {
      "website": "styles/website.css",
      "pdf": "styles/ebook.css"
    },
    "gitbook": ">=3.1.0",
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "bibtex": {
            "bibtex-path": "references.bib",
            "references-url": "/references.html",
            "format": "acm"
        },
        "image-captions": {
            "attributes": { "width": "450" },
            "variable_name": "figures",
            "caption": "<strong>Figure _BOOK_IMAGE_NUMBER_:</strong> _CAPTION_",
            "list_caption": "_CAPTION_",
            "images": {
                "10.1": {
                    "x-axis": "Questions Attempted",
                    "y-axis": "Students"
                },
                "10.2": {
                    "y-axis": "Submissions",
                    "x-axis": "Date"
                },
                "10.3": {
                    "y-axis": "Submissions",
                    "x-axis": "Date"
                },
                "10.4": {
                    "y-axis": "Submissions",
                    "x-axis": "Date"
                },
                "10.5": {
                    "y-axis": "Submissions",
                    "x-axis": "Hour of Day (24-hours)"
                },
                "10.6": {
                    "y-axis": "Submissions",
                    "x-axis": "Day of week"
                },
                "10.7": {
                    "y-axis": "Students",
                    "x-axis": "Times Submitted"
                },
                "10.8": {
                    "y-axis": "Students",
                    "x-axis": "Times Submitted"
                }
            }
        }
    },
    "variables": {},
    "structure": {
        "readme": "abstract.md"
    },
    "pdf": {
        "ebookConvertOptions": {
            "--pdf-add-toc": true,
            "--embed-all-fonts": true
        },
        "toc": true,
        "pageNumbers": true,
        "fontFamily": "Times New Roman",
        "fontSize": 12,
        "paperSize": "letter",
        "margin": {
            "right": 90,
            "left": 90,
            "top": 90,
            "bottom": 90
        }
    }
}