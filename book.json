{
    "title": "Î» - An Autograder for Snap!",
    "author": "Michael Ball",
    "description": "An autograder for the Snap! programming language, my UC Berkeley Masters' Thesis.",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "anchors",
        "indexes",
        "bibtex-citation-michael",
        "image-captions"
    ],
    "pluginsConfig": {
        "bibtex": {
            "bibtex-path": "references.bib",
            "references-url": "/references.html",
            "format": "acm"
        },
        "image-captions": {
            "variable_name": "figures",
            "caption": "<strong>Figure _BOOK_IMAGE_NUMBER_:</strong> _CAPTION_",
            "list_caption": "_CAPTION_"
        }
    },
    "structure": {
        "readme": "abstract.md"
    },
    "pdf": {
        "toc": true,
        "pageNumbers": true,
        "fontSize": 12,
        "paperSize": "letter",
        "chapterMark": "pagebreak",
        "pageBreaksBefore": "/",
        "margin": {
            "right": 90,
            "left": 90,
            "top": 90,
            "bottom": 90
        },
        "headerTemplate": "",
        "footerTemplate": "_PAGENUM_",
        "ebookConvertOptions": {
            "--chapter": "/",
            "--embed-all-fonts": true
        }
    }
}