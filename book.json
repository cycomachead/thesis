{
    "title": "Î» - An Autograder for Snap!",
    "author": "Michael Ball",
    "description": "An autograder for the Snap! programming language, my UC Berkeley Masters' Thesis.",
    "plugins": [
        "bibtex-citation-michael",
        {
            "name": "image-captions",
            "version": "git+https://github.com/todvora/gitbook-plugin-image-captions.git"
        }
    ],
    "styles": {
      "website": "styles/website.css",
      "pdf": "styles/ebook.css"
    },
    "gitbook": ">=3.1.0",
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "bibtex": {
            "bibtex-path": "references.bib",
            "references-url": "/references.html",
            "format": "acm"
        },
        "image-captions": {
            "attributes": { "width": "450" },
            "variable_name": "figures",
            "caption": "<strong>Figure _BOOK_IMAGE_NUMBER_:</strong> _CAPTION_",
            "list_caption": "_CAPTION_",
            "images": {
                "8.4": {
                    "width": "600"
                },
                "8.5": {
                    "width": "600",
                    "caption": "<strong>Figure _BOOK_IMAGE_NUMBER_:</strong> _CAPTION_ (Greyed out items mean that the user hasn't done something to enable that action.)"
                }
            }
        }
    },
    "variables": {
        "TEST": "hello"
    },
    "structure": {
        "readme": "abstract.md"
    },
    "pdf": {
        "toc": true,
        "pageNumbers": true,
        "fontFamily": "Times New Roman",
        "fontSize": 12,
        "paperSize": "letter",
        "margin": {
            "right": 90,
            "left": 90,
            "top": 90,
            "bottom": 90
        }
    }
}